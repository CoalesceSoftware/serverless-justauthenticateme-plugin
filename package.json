{
  "name": "serverless-justauthenticateme-plugin",
  "version": "1.0.0",
  "description": "Serverless Framework Plugin for https://www.justauthenticate.me",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "author": "Grant Orndorff",
  "license": "MIT",
  "keywords": [
    "justauthenticateme",
    "authentication",
    "auth",
    "serverless",
    "apigateway",
    "lambda"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/node": "^13.9.5",
    "husky": "^4.0.6",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "fs-extra": "^9.0.0"
  },
  "peerDependencies": {
    "justauthenticateme-apigateway-auth": "0.0.8"
  }
}
